{
	"compilerOptions": {
		"target": "ESNext",
		"module": "CommonJS",
		"lib": ["ESNext"],
		"sourceMap": true,
		"rootDir": "./",
		"outDir": ".build",
		"strict": true,
		"baseUrl": "./src",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["./src"],
	"exclude": ["./node_modules"]
}
